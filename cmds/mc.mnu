e   Editor
    e %f
n   Notepad
    start notepad %f
o   Open file
    start %f
f   Diff
    dif %f %D\%f & pause
d   Diff UI
    wm %f %D\%f
D   Diff Dir
    wm %d %D
p   Paste filename
    echo %f|gc
g   Git diff
    start "Git-Diff" cmd /C git diff %f
t   Git difftool
    start "Git-Diff" cmd /C git difftool %f
j   ES6-editor
    ejs %f
h   PHP-editor
    ephp %f
l   Edit Selected
    loadfiles %t
